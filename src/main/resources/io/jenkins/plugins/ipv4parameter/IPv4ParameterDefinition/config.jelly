<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:entry title="${%Name}" field="name">
    <f:textbox />
  </f:entry>
  <f:entry title="${%Description}" field="description">
    <f:textarea />
  </f:entry>
  <f:entry title="${%Default Value}" field="defaultValue">
    <f:textbox default="192.168.1.1" />
  </f:entry>
</j:jelly>